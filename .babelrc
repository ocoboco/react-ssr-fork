{
  "presets": [
    "@babel/stage-3",
    "@babel/react",
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "plugins": [
    "@babel/external-helpers",
    ["module-resolver", {
      "extensions": [".js", ".jsx"],
      "root": ["./src"]
    }],
    "@babel/plugin-proposal-class-properties"
  ]
}